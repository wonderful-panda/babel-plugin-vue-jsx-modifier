{
  "name": "babel-plugin-vue-jsx-modifier",
  "version": "0.1.1",
  "description": "Support modifiers(sync/capture/passive/once) in JSX",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build-all": "npm-run-all check-format build test",
    "check-format": "prettier --list-different src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "test": "jest"
  },
  "repository": {
    "url": "https://github.com/wonderful-panda/babel-plugin-vue-jsx-modifier.git",
    "type": "git"
  },
  "keywords": [
    "Vue",
    "JSX",
    "babel",
    "modifier"
  ],
  "author": "Iwata Hidetaka",
  "license": "MIT",
  "devDependencies": {
    "@types/babel-core": "^6.25.3",
    "@types/babel-plugin-syntax-jsx": "^6.18.0",
    "@types/babel-types": "^7.0.1",
    "@types/jest": "^23.3.5",
    "@types/node": "^10.11.7",
    "babel-core": "^6.26.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-types": "^6.26.0",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "ts-jest": "^23.10.4",
    "typescript": "^2.8.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "node",
      "json"
    ]
  }
}
