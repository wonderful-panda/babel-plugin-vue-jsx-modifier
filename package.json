{
  "name": "babel-plugin-vue-jsx-modifier",
  "version": "1.0.1",
  "description": "Support modifiers(sync/capture/passive/once) in JSX",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build-all": "npm-run-all check-format build test",
    "check-format": "prettier --list-different src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "test": "jest"
  },
  "repository": {
    "url": "https://github.com/wonderful-panda/babel-plugin-vue-jsx-modifier.git",
    "type": "git"
  },
  "keywords": [
    "Vue",
    "JSX",
    "babel",
    "modifier"
  ],
  "author": "Iwata Hidetaka",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@types/babel__core": "^7.0.1",
    "@types/babel__generator": "^7.0.0",
    "@types/babel__template": "^7.0.0",
    "@types/babel__traverse": "^7.0.0",
    "@types/jest": "^23.3.5",
    "@types/node": "^10.11.7",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "lodash": "^4.17.14",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "regenerator-runtime": "^0.12.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "node",
      "json"
    ]
  }
}
